  <script>
    $( document ).ready(function() {
      $("#AboutMe").click(function(){
        if($("#AboutButtonText").text()==" My Contact")
        {
          $("#row1").css("display", "none")
          $("#row2").css("display", "none")
          $("#rowAbout").css("display", "block")
          $("#AboutButtonText").text(" Hide Contact")
        }
        else{
          $("#row1").css("display", "block")
          $("#row2").css("display", "none")
          $("#rowAbout").css("display", "none")
          $("#AboutButtonText").text(" My Contact")

        }
      })

      $("#proj").click(function(){
        var proj = $("#folder")
        if($("#ProjText").text()==" See Projects")
        {
          proj.removeClass("glyphicon glyphicon-folder-close")
          proj.addClass("glyphicon glyphicon-folder-open")
          $("#ProjText").text(" Hide Projects")
          $("#row1").css("display","none")
          $("#row2").css("display","block")
          $("#rowAbout").css("display","none")
        }
        else
        {
          proj.removeClass("glyphicon glyphicon-folder-open")
          proj.addClass("glyphicon glyphicon-folder-close")
          $("#ProjText").text(" See Projects")
          $("#row1").css("display","block")
          $("#row2").css("display","none")
          $("#rowAbout").css("display","none")
        }


      })
      $("#NextProj1").click( function(){
        $("#row3").css("display","none")
        $("#row4").css("display","block")
        $("#row5").css("display","none")
        $("#row6").css("display","none")

      })
      $("#PrevProj1").click( function(){
        $("#row4").css("display","none")
        $("#row3").css("display","block")
        $("#row5").css("display","none")
        $("#row6").css("display","none")

      })
      $("#NextProj2").click( function(){
        $("#row4").css("display","none")
        $("#row3").css("display","none")
        $("#row5").css("display","block")
        $("#row6").css("display","none")


      })
      $("#NextProj3").click( function(){
        $("#row4").css("display","none")
        $("#row3").css("display","none")
        $("#row5").css("display","none")
        $("#row6").css("display","block")


      })
      $("#Hide").click(function(){
        var div = $("img")
        var divForm = $("#FormFill")
        var imgDiv = $("#Images")
        div.slideToggle();
        if($(this).text()=="Hide Images!!") {
          $(this).text('Show Images!!')
          divForm.addClass("col-md-10")
          divForm.removeClass("col-md-8")
          imgDiv.addClass("col-md-0")
          imgDiv.removeClass("col-md-3")
          if (!("Notification" in window)) {
            alert("This browser does not support desktop notification");
          }
          // Let's check whether notification permissions have already been granted
          else if (Notification.permission === "granted") {
            // If it's okay let's create a notification
            var n = new Notification("You could click on the images and redirect..", {iconUrl:'https://media.licdn.com/mpr/mpr/shrinknp_400_400/p/8/005/0b8/2a1/1e2d049.jpg'});
            setTimeout(n.close.bind(n), 3000);
          }
          // Otherwise, we need to ask the user for permission
          else if (Notification.permission !== 'denied') {
            Notification.requestPermission(function (permission) {
              // If the user accepts, let's create a notification
              if (permission === "granted") {
                var n = new Notification("You could click on the images and redirect..", {iconUrl:'https://media.licdn.com/mpr/mpr/shrinknp_400_400/p/8/005/0b8/2a1/1e2d049.jpg'});
                setTimeout(n.close.bind(n), 3000);
              }
            });
          }

        }
        else{
          $(this).text("Hide Images!!")
          imgDiv.removeClass("col-md-0")
          imgDiv.addClass("col-md-3")
          divForm.removeClass("col-md-10")
          divForm.addClass("col-md-8")

        }

      })

      $("#Crazy").click(function()
      {
        var div = $("div");
        div.css("display","block")
        $("#row4").css("display","none")
        $("#row5").css("display","none")
        $("#row6").css("display","none")


      })

    });
  </script>
